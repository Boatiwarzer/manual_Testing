                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="100.0" prefWidth="200.0" text="Test Result">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="testResultNameLabel" contentDisplay="CENTER" prefHeight="100.0" prefWidth="400.0" text="Name">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="testResultIDLabel" contentDisplay="CENTER" prefHeight="100.0" prefWidth="150.0" text="ID">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="120.0" text="Test No.">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="onTestNo" alignment="TOP_LEFT" onKeyTyped="#GenerateDate" prefHeight="30.0" prefWidth="450.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="120.0" text="Test Script ID">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="onTestscriptIDComboBox" onAction="#onTestscriptIDComboBox" prefHeight="30.0" prefWidth="450.0" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="60.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label alignment="TOP_LEFT" prefHeight="30.0" prefWidth="120.0" text="Description">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="onDescription" alignment="TOP_LEFT" prefHeight="60.0" prefWidth="450.0" text="Label" wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="120.0" text="Actor">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="onActor" alignment="TOP_LEFT" onAction="#onActor" prefHeight="30.0" prefWidth="450.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="90.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="120.0" text="Test Steps">
                     <font>
                        <Font size="16.0" />
